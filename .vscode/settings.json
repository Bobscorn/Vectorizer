{
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "gtest-adapter.debugConfig": "linux build then debug",
    "gtest-adapter.supportLocation": false,
    "telemetry.enableTelemetry": false,
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**", 
        "~/.conan/data/**"
    ],
    "C_Cpp.default.compilerPath": "/usr/bin/gcc",
    "files.associations": {
        "*.tcc": "c",
        "cstdlib": "c",
        "memory": "c",
        "type_traits": "c",
        "xtr1common": "c",
        "xmemory": "cpp",
        "algorithm": "cpp",
        "xutility": "cpp",
        "*.rh": "cpp",
        "compare": "c",
        "utility": "c",
        "cmath": "c",
        "iterator": "cpp",
        "list": "cpp",
        "vector": "cpp",
        "xstring": "cpp",
        "svg.h": "c",
        "tears.h": "c"
    },
    "C_Cpp.intelliSenseEngineFallback": "Enabled"
}