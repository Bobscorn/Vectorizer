    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch Tests",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\C_part\\build\\windows\\x64\\debug\\tests.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}\\C_part\\build\\windows\\x64\\debug",
            "environment": [],
            "console": "internalConsole",
            "requireExactSource": false,
        },
        {
            "name": "(Windows) Launch one test",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\C_part\\build\\windows\\x64\\debug\\tests.exe",
            "args": ["nsvg_to_svg"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}\\C_part\\build\\windows\\x64\\debug",
            "environment": [],
            "console": "internalConsole",
            "requireExactSource": false
        },
        {
            "name": "(Windows) Launch Vectorizer against input.png",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\C_part\\build\\windows\\x64\\debug\\vectorizer.exe",
            "args": ["../../../../discord-v/input.png", "output.png", "4", "0"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}\\C_part\\build\\windows\\x64\\debug",
            "environment": [],
            "console": "internalConsole",
            "requireExactSource": false
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch",
            "args": [],
            "program": "${workspaceFolder}/Rust_part/target/debug/bot.exe",
            "windows": {
                "program": "${workspaceFolder}/Rust_part/target/debug/bot.exe"
            },
            "cwd": "${workspaceFolder}",
            "stopOnEntry": false,
            "sourceLanguages": ["rust"],
            "sourceMap": {
              "/rustc/*": "${env:HOME}/.rustup/toolchains/stable-x86_64-apple-darwin/lib/rustlib/src/rust"
            }
          },
    ]
}
